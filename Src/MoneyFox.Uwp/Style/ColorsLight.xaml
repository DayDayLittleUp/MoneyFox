<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  Set Accent color manually  -->
    <Color x:Key="SystemAccentColor">#E45E8D</Color>
    <Color x:Key="ColorPrimary">#FF314A9B</Color>
    <Color x:Key="ColorPrimaryDark">#FF00165A</Color>

    <!--<Color x:Key="ColorCustomBackground">#c2c2c2</Color>-->
    <Color x:Key="ColorCustomBackground">#EFF2F5</Color>
    <Color x:Key="ContentDialogBackgroundColor">#FFEFF2F5</Color>

    <Color x:Key="ColorSplashScreenBackground">#FFEFEADD</Color>

    <Color x:Key="AppForegroundColorPrimary">#FF2B343D</Color>
    <Color x:Key="AppForegroundColorSecondary">#908e8c</Color>

    <Color x:Key="AppMenuForeground">White</Color>
    <Color x:Key="ListItemColor">White</Color>
    <Color x:Key="WarningColor">#FFFF4500</Color>

    <Color x:Key="BorderColor">#b3b2b0</Color>
</ResourceDictionary>